<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qy.identity.app.infrastructure.persistence.mybatis.mapper.UserMapper">

    <select id="getEmployeeDO" resultType="com.qy.identity.app.application.dto.EmployeeDO">
        select * from  ark_uims_org_employee where 1=1 and phone = #{username} and  job_status_id = 1 and is_deleted = 0 limit 1
    </select>
</mapper>
