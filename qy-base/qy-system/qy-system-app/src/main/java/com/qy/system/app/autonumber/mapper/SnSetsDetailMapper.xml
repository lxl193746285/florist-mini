<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qy.system.app.autonumber.mapper.SnSetsDetailMapper">

    <delete id="deleteBySnSetsId" parameterType="com.qy.system.app.autonumber.entity.SnSetsDetailEntity">
        update st_system_noitem
        set is_deleted = 1 , deletor_id = #{deletorId} , deletor_name = #{deletorName} , delete_time = #{deleteTime}
        where noid = #{noid} and company_id = #{companyId}
    </delete>
    <select id="getAutoNumber" resultType="java.lang.String">
        SELECT f_sys_getno2(#{noId}, #{companyId}, #{userId}) as order_no
    </select>

</mapper>
