<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qy.wf.defNodeRelation.mapper.DefNodeRelationMapper">

    <delete id="deleteByNodeCode">
        delete from wf_def_node_relation where node_code in
        <foreach collection="nodeCodeList" item="nodeCode" open="(" separator="," close=")">
            #{nodeCode}
        </foreach>
    </delete>
</mapper>
