<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qy.wf.nodetable.mapper.NodeTableMapper">

    <delete id="deleteByNodeCode">
        delete from wf_def_node_table where node_code in
        <foreach collection="list" item="nodeCode" open="(" separator="," close=")">
            #{nodeCode}
        </foreach>
    </delete>
    <delete id="deleteByWfId">
        delete from wf_def_node_table where wf_id = #{wfId}
    </delete>
</mapper>
